{
    "presets": [
        ["env", {
            "targets": {
              "browsers": ["last 2 versions", "safari >= 7"]
            }
        }],
        "stage-0",
        "react"
    ],
    "plugins": [
        ["transform-runtime", {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
        }],
        "react-hot-loader/babel",
        "transform-decorators-legacy"
    ]
}